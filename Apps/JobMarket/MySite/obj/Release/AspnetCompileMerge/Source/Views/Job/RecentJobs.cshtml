@using MySite.Helpers
@model MySite.Models.JobSearchResultModel

@if (Model.JobsList.HasData())
{
    <div class="job-listings-sec no-border">
        @foreach (var item in Model.JobsList)
        {
            @Html.Partial("_JobResultItem", item)
        }
    </div>
}

