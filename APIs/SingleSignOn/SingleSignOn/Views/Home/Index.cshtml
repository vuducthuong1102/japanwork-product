@model SingleSignOn.Models.ResponseWebAuthLoginModel

<div class="jumbotron">
    <h1>Welcome to SingleSignOn </h1>        
    <br />
    @if(Model != null && !string.IsNullOrEmpty(Model.TokenKey))
    {
        <pre>
            <code>
               + Login successfully.Your current token key is: @Model.TokenKey
               + URLReturn: @Model.URLReturn
            </code>
        </pre>
    }

</div>

